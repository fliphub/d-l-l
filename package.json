{
  "fliplog": {
    "use": [
      "cli"
    ],
    "debug": false
  },
  "name": "d-l-l",
  "version": "0.1.0",
  "description": "easy, automatic, optimized dll config handler for webpack",
  "main": "index.js",
  "keywords": [
    "webpack",
    "dll",
    "dynamically-linked-library",
    "easy",
    "automatic",
    "config",
    "optimize"
  ],
  "scripts": {
    "flip": "cd examples/frontend && npm run build -- --verbose",
    "example-web": "cd examples/frontend && npm i && npm run build -- --verbose",
    "example-node": "cd ../node && npm i && npm run build -- --verbose && npm run exec",
    "examples": "npm run example-web && npm run example-node",
    "test": "ava 'test/*.js' --verbose"
  },
  "todoDependencies": {
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "webpack-assetgraph-plugin": "^0.1.0",
    "webpack-merge": "^4.1.0",
    "app-root-path": "^2.0.1"
  },
  "dependencies": {
    "chain-able": "^1.0.0",
    "flipcache": "*",
    "flipfile": "0.0.12",
    "fliphash": "0.0.1",
    "fliplog": "^0.2.7",
    "globby": "^6.1.0"
  },
  "devDependencies": {
    "ava": "0.19.1",
    "pify": "^2.3.0",
    "webpack": "*"
  },
  "license": "MIT",
  "author": "James <aretecode@gmail.com>",
  "homepage": "https://github.com/fliphub/d-l-l#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fliphub/d-l-l.git"
  },
  "bugs": {
    "url": "https://github.com/fliphub/d-l-l/issues"
  }
}
